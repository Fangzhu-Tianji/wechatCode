/**app.wxss**/
text{ font-family: MicroSoft Yahei; font-size: 24rpx; color: #666; }
/* 浮动 */
.fl { float: left;}
.fr { float: right;}
.clearfix { zoom: 1; }
.clearfix:before,
.clearfix:after { content: ''; display: table; }
.clearfix:after { clear: both; } /*清除浮动*/
/* 多出的文字用省略号代替 */
.texthide {text-overflow:ellipsis; overflow:hidden; white-space:nowrap;}
.texthides {display: -webkit-box !important; overflow: hidden;text-overflow: ellipsis;word-break: break-all;
-webkit-box-orient: vertical;-webkit-line-clamp: 3; /*数字几就代表几行*/ }

/* 引入字体图标 */
@font-face {font-family: "iconfont";
  src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff')
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:24rpx;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

